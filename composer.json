{
  "name": "wemersonrv/input-mask",
  "description": "A Laravel Nova custom field text with masks on input",
  "keywords": [
    "laravel",
    "nova",
    "field",
    "mask"
  ],
  "license": "GPLv3",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Wemersonrv\\InputMask\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wemersonrv\\InputMask\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
